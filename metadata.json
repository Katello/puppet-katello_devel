{
  "name": "katello-katello_devel",
  "version": "1.0.0",
  "author": "Katello",
  "summary": "Handles deploying and managing a Katello + Foreman development setup from git",
  "license": "GPL-3.0+",
  "source": "https://github.com/Katello/puppet-katello_devel.git",
  "project_page": "https://github.com/Katello/puppet-katello_devel",
  "issues_url": "http://projects.theforeman.org/projects/katello/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    },
    {
      "name": "puppet/extlib",
      "version_requirement": ">= 0.10.4 < 1.0.0"
    },
    {
       "name": "puppetlabs-apache",
       "version_requirement": ">= 1.0.0 < 2.0.0"
    },
    {
       "name": "puppetlabs-postgresql",
       "version_requirement": ">= 3.0.0"
    },
    {
       "name": "puppetlabs-mongodb"
    },
    {
       "name": "puppetlabs-vcsrepo"
    },
    {
       "name": "katello-katello",
       "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
       "name": "katello-certs",
       "version_requirement": ">= 1.0.0 < 2.0.0"
    },
    {
       "name": "katello-qpid",
       "version_requirement": ">= 0.1.0"
    },
    {
       "name": "katello-candlepin",
       "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
       "name": "katello-pulp",
       "version_requirement": ">= 4.0.0 < 5.0.0"
    },
    {
       "name": "katello-crane",
       "version_requirement": ">= 2.0.0 < 3.0.0"
    },
    {
       "name": "katello-common",
       "version_requirement": ">= 1.0.0 < 2.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.2.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "7"
      ]
    }
  ]
}
